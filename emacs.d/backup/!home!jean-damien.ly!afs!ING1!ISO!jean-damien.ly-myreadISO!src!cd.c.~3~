#include "myiso.h"

void cd_command(char **fp, char *read, int index)
{
    if (strcmp(read, "") == 0)
        *fp = get_mmap();
    else
    {
        fp = fp;
        read = read;
        index = index;
        void *tmp = check_argument(*fp, read, index);
        *fp = tmp;
        /*printf("cc: %p\n", tmp);
        struct iso_dir *iso = tmp;
        printf("now in %s %d\n", read, iso->idf_len);
        tmp = get_mmap() + iso->data_blk.le * ISO_BLOCK_SIZE;
        printf("bb: %p\n", tmp);
        *fp = tmp;
        printf("aa: %p\n", *fp);*/
    }
}