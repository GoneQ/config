#include "myiso.h"

void tree_command(char **fp, char *read, int index)
{
    void *root = check_argument(*fp, read, index);
    if (root = NULL)
        return;
    struct iso_dir *newid = root;
    int count = 0;
    while (newid->file_size.le != 0)
    {
        char *filename = get_name(newid, count++);
        int len = get_len(newid->idf_len, filename);
        if (filename[len - 2] == ";
    }
}