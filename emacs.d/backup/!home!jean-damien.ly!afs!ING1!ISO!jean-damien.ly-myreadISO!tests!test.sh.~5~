#!/bin/sh
echo
stmy="help\nquit\n"

while IFS='' read line; do
    T=$(tr -d '\0' < ./my_read_iso myiso.iso <<< $line 2>/dev/null)
    expected=$(tr -d '\0' <./provided/my_read_iso myiso.iso <<< $line 2>/dev/null)
    if [ "$T" = "$expected" ]; then
        echo -e "$line \e[1m \e[0;32mOK\033[0m"
    else
        echo -e "$line \e[1m \e[0;31mFAILED\033[0m"
    fi
done < tests/command

