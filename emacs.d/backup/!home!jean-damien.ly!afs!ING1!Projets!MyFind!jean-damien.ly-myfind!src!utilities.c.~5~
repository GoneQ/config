#include "myfind.h"

size_t my_strlen(char *s)
{
    size_t i = 0;
    for (; s[i] != '\0'; i++);
    return i;
}

char *my_strncpy(char *d, char *s, size_t n)
{
    size_t i = 0;
    for (; ((i < n) && (s[i] != '\0')); i++)
        d[i] = s[i];
    d[i] = '\0';
    return d;
}

int my_strcmp(char *a, char *b)
{
    int i = 0;
    while ((a[i] != '\0') && (b[i] != '\0') && (a[i] == b[i]))
    {
        i++;
    }
    if (a[i] != b[i])
        return 1;
    return 0;
}

int check_type(unsigned char d)
{
    int type = d;
    int bin[32];
    for (int j = 0; j < 32; j++)
        bin[j] = 0;
    int i = 31;
    while (type > 0)
    {
        bin[i] = type % 2;
        type = type / 2;
        i--;
    }
    if (bin[29] == 1)
        return 1;
    if (bin[26] == 1)
        return 0;
    return -1;
}