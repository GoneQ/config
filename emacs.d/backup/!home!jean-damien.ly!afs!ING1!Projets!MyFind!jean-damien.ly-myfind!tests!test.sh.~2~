#!/bin/sh
echo
stmy="help\nquit\n"


while IFS='' read line; do
    T=$(tr -d '\0' < ./myfind $line 2>/dev/null)
    expected=$(tr -d '\0' < find $line 2>/dev/null)
    if [ "$T" = "$expected" ]; then
        printf "%-20s | \e[1m \e[0;32m%-10s\033[0m\n" "$line" "SUCCEED"
    else
        printf "%-20s | \e[1m \e[0;31m%-10s\033[0m\n" "$line" "FAILED"
    fi
done < tests/command

